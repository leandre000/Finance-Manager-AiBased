# Application
PORT=3000
NODE_ENV=development

# PostgreSQL Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=fima
POSTGRES_USER=fima
POSTGRES_PASSWORD=secret

# RabbitMQ
RABBITMQ_URL=amqp://fima:secret@localhost:5672

# MinIO Object Storage
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=fima
MINIO_SECRET_KEY=secret123
MINIO_BUCKET=receipts

# JWT Authentication
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# AI/ML Service (optional)
OPENAI_API_KEY=your-openai-api-key
